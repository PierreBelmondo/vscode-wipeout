{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "es2019"],
    "tsBuildInfoFile": "tsconfig.browser.tsbuildinfo",
    "baseUrl": "webviews",
    "paths": {
      "@core/*": ["../core/*"],
      "@compat/*": ["../compat/browser/*"]
    }
  },
  "include": ["compat/browser", "core", "webviews"],
  "exclude": ["compat/node", "node_modules", "src"]
}
